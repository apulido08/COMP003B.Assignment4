*@model VideoGame

@{
    ViewData["Title"] = "Game Details";
    Layout = "_Layout";
}

<h2>@Model.Title</h2>
<p><strong>Genre:</strong> @Model.Genre</p>
<p><strong>Description:</strong> @Model.Description</p>
<p><strong>Release Date:</strong> @Model.ReleaseDate.ToString("MM/dd/yyyy")</p>

